    <VBox layoutX="47.0" layoutY="98.0" spacing="10.0">

        <ListView fx:id="listView" prefHeight="220.0" prefWidth="443.0">
            <items>
                <FXCollections fx:factory="observableArrayList" />
            </items>
        </ListView>
    </VBox>
    <Button layoutX="269.0" layoutY="340.0" onAction="#ajouterAppartement" text="Ajouter Apartement" />
    <Button layoutX="121.0" layoutY="339.0" onAction="#modiferAppartement" prefHeight="26.0" prefWidth="134.0" text="Modifier Appartement" />
    <Button layoutX="3.0" layoutY="340.0" onAction="#gererFacture" text="Gerer Facture" />
    <Button layoutX="396.0" layoutY="340.0" onAction="#supprimerAppartement" text="Supprimer Appartement" />

    <Button defaultButton="true" layoutX="400.0" layoutY="30.0" onAction="#actualiser" text="Actualiser" textFill="#241584" />

</AnchorPane>
