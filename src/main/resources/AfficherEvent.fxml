<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.AfficherEvent">
    <children>
        <ImageView fitHeight="600.0" fitWidth="800.0">
            <image>
                <Image url="@image/modification des events.png" /> <!-- Assurez-vous d'avoir l'image correcte et le chemin correct -->
            </image>
        </ImageView>
        <ListView fx:id="listeEvents" layoutX="60.0" layoutY="118.0" prefHeight="312.0" prefWidth="651.0" />
        <HBox layoutX="46.0" layoutY="366.0" spacing="20.0" /> <!-- Vous pouvez ajouter d'autres éléments ici si nécessaire -->
        <Button fx:id="boutonGerer" layoutX="81.0" layoutY="453.0" mnemonicParsing="false" text="Gerer" />
        <!-- Ajoutez d'autres boutons ou éléments au besoin -->
    </children>
</AnchorPane>
