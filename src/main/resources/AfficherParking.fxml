<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.AfficherParking">
    <children>
        <ImageView fitHeight="600.0" fitWidth="800.0">
            <image>
                <Image url="@image/listeParking.png" />
            </image>
        </ImageView>
        <!-- Bouton "Gérer les voitures" -->
        <Button fx:id="boutonGererVoitures" layoutX="46.0" layoutY="97.0" mnemonicParsing="false" onAction="#ouvrirAffichervoiture" opacity="0.0" prefHeight="44.0" prefWidth="129.0" text="Gérer les voitures" />


        <ListView fx:id="listeParkings" layoutX="269.0" layoutY="119.0" prefHeight="312.0" prefWidth="458.0" />
        <HBox layoutX="46.0" layoutY="366.0" spacing="20.0" />
        <Button fx:id="boutonAjouter" layoutX="269.0" layoutY="456.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="86.0" text="Ajouter" />

    </children>
</AnchorPane>
